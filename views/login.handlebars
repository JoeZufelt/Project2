<div class="container">
    <div class="row">
      <div class="col-md-6 col-md-offset-3">
        <h2>Login Form</h2>
        <form id="login"class="login">
          <div class="form-group">
            <label for="email">Email address</label>
            <input id="emailinput" type="email" class="form-control" placeholder="Email">
          </div>
          <div class="form-group">
            <label for="password">Password</label>
            <input id="passwordinput" type="password" class="form-control"  placeholder="Password">
          </div>
          <button type="submit" class="btn btn-default">Login</button>
        </form>
        <br />
        <p>Or sign up <a href="/signup">here</a></p>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <input id="password" type="password" class="validate">
          <label for="password">Password</label>
        </div>
      </div>
      <div class="button center-align">
      <a class="waves-effect waves-light btn-large login-btn">Login</a>
      </div>
    </form>
  </div>
