<div id="modal1" class="modal">
    <div class="modal-content">
      <h4>Find a mate!</h4>   <button  class="modal-close">X</button>
      <form action="/movies" method="POST">
      <div id="modalContainer">
         <div class="input-field col s12">
          <input name="modalname"id="modalname" type="text" class="validate">
          <label for="modalname">First and last name</label>
        </div>
         <div class="input-field col s12">
          <input name="modalemail" id="modalemail" type="text" class="validate">
          <label for="modalemail">Email</label>
        </div>
         <div class="input-field col s12">
          <input name="modalzipcode" id="modalzipcode" type="text" class="validate">
          <label for="modalzipcode">Zipcode</label>
        </div>
        <div class="input-field col s12">
          <input name="modaltime" id="modaltime" type="text" class="validate">
          <label for="modaltime">Movie time</label>
        </div>
         <div class="input-field col s12">
          <input name="modalpreference"id="modalpreference" type="text" class="validate">
          <label for="preference">Movie preference</label>
        </div>

      </div>
     
    </div>
    <div class="modal-footer">
       <center><input type="submit" class="modal-close" value="Post"></center>
    </div>
    </form>
  </div>
 <a class="waves-effect waves-light btn modal-trigger" href="#modal1">Create a Post
